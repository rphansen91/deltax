{
  "name": "redux-delta",
  "version": "0.0.5",
  "description": "Helper methods and cli to remove the boilerplate of Redux project setup and development.",
  "main": "./dist/package/index.js",
  "scripts": {
    "test": "jest",
    "dev": "concurrently \"tsc -w\" \"nodemon example.js\"",
    "docs": "http-server -p 8080 -c-1 ./dist/docs",
    "build:docs": "rm -rf ./dist/docs && typedoc --options typedoc.json",
    "build:package": "rm -rf ./dist/package && tsc && node make-package.js"
  },
  "bin": {
    "redux-delta": "./cli/cli.js"
  },
  "keywords": [
    "redux",
    "middleware"
  ],
  "author": "Ryan P. Hansen",
  "repository": {
    "type": "git",
    "url": "https://github.com/rphansen91/redux-delta"
  },
  "license": "ISC",
  "dependencies": {
    "commander": "^2.16.0",
    "ncp": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "concurrently": "^3.6.1",
    "jest": "^23.4.2",
    "node-fetch": "^2.2.0",
    "redux": "^4.0.0",
    "ts-jest": "^23.1.3",
    "typescript": "^3.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true,
    "testURL": "http://localhost/",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
