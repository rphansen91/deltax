{
  "name": "redux-delta",
  "version": "0.0.0-alpha.1",
  "description": "Helper methods and cli to remove the boilerplate of Redux project setup and development.",
  "main": "./dist/package/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"tsc -w\" \"nodemon example.js\"",
    "docs": "http-server -p 8080 -c-1 ./docs",
    "build:docs": "typedoc --options typedoc.json",
    "build:lib": "rm -rf ./lib && tsc"
  },
  "bin": {
    "redux-delta": "./cli/cli.js"
  },
  "keywords": [
    "redux",
    "middleware"
  ],
  "author": "Ryan P. Hansen",
  "repository": {
    "type": "git",
    "url": "https://github.com/rphansen91/redux-delta"
  },
  "license": "ISC",
  "dependencies": {
    "commander": "^2.16.0",
    "ncp": "^2.0.0"
  },
  "devDependencies": {
    "redux": "^4.0.0",
    "concurrently": "^3.6.1",
    "node-fetch": "^2.2.0"
  }
}
